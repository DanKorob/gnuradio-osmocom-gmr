<?xml version="1.0"?>
<block>
 <name>RACH Detection</name>
 <key>rach_detect</key>
 <category>GMR-1</category>
 <import>from gnuradio import gmr1</import>
 <make>gmr1.rach_detect($sps, $burst_window, $freq_bias, $len_tag_key)</make>
 <param>
   <name>Samples-per-Symbol</name>
   <key>sps</key>
   <type>int</type>
 </param>
 <param>
   <name>Burst window</name>
   <key>burst_window</key>
   <type>int</type>
 </param>
 <param>
   <name>Frequency bias</name>
   <key>freq_bias</key>
   <type>float</type>
 </param>
 <param>
   <name>Length Tag Name</name>
   <key>len_tag_key</key>
   <value>packet_len</value>
   <type>string</type>
 </param>
 <sink>
   <name>in</name>
   <type>complex</type>
 </sink>
 <source>
   <name>burst</name>
   <type>complex</type>
 </source>
</block>
